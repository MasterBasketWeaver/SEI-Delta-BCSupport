enum 50000 "BA Approval Status"
{
    Extensible = true;
    value(0; " ")
    {
        Caption = ' ';
    }
    value(1; "Pending")
    {
        Caption = 'Pending';
    }
    value(2; "Released")
    {
        Caption = 'Released';
    }
    value(3; "Cancelled")
    {
        Caption = 'Cancelled';
    }
    value(4; "Rejected")
    {
        Caption = 'Rejected';
    }
}